# OMEN Environment Variables
# Copy this file to .env and fill in your API keys

# Server Configuration
OMEN_BIND=0.0.0.0
OMEN_PORT=8080

# Database
OMEN_DB_URL=sqlite:///data/omen.db
# OMEN_REDIS_URL=redis://localhost:6379

# Provider API Keys
# OpenAI
# OMEN_OPENAI_API_KEY=sk-...

# Anthropic Claude
# OMEN_ANTHROPIC_API_KEY=sk-ant-...

# Google Gemini
# OMEN_GOOGLE_API_KEY=...

# xAI Grok
# OMEN_XAI_API_KEY=xai-...

# Azure OpenAI
# OMEN_AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
# OMEN_AZURE_OPENAI_API_KEY=...

# Ollama Endpoints (comma-separated)
# OMEN_OLLAMA_ENDPOINTS=http://localhost:11434,http://ollama-server:11434

# Routing Configuration
# OMEN_ROUTER_PREFER_LOCAL_FOR=code,regex,tests
# OMEN_BUDGET_MONTHLY_USD=150
# OMEN_SOFT_LIMIT_ANTHROPIC=70
# OMEN_SOFT_LIMIT_OPENAI=70

# Authentication
# OMEN_MASTER_KEY=your-secret-key